<%= form_for [@company, @product, @review] do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :description %>
  <%= f.text_field :description %>
</p>
<p>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
</p>
<p>
  <%= f.submit class: 'btn btn-primary' %>
  <%= link_to 'Back', company_product_path(@company, @product), class: 'btn btn-default' %>
</p>
<% end %>
