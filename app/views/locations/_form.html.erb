<%= form_for [@company, @location] do |f| %>

<p>
  <%= f.label :city %>
  <%= f.text_field :city %>
</p>
<p>
  <%= f.label :street_name %>
  <%= f.text_field :street_name %>
</p>
<p>
  <%= f.label :state %>
  <%= f.text_field :state %>
</p>
<p>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
</p>
<p>
  <%= f.label :country %>
  <%= f.text_field :country %>
</p>
<p>
  <%= f.submit class: 'btn btn-primary' %>
  <%= link_to 'Back', companies_path, class: 'btn btn-default' %>
</p>

<% end %>
